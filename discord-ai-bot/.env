#######################################
# DISCORD BOT CONFIGURATION
#######################################

# REQUIRED: Get your bot token from Discord Developer Portal
# Format: DISCORD_TOKEN=your-actual-bot-token-here
DISCORD_TOKEN=your-bot-token-here

# Command prefix for bot commands (default: !)
COMMAND_PREFIX=!

# Comma-separated list of admin user IDs
# Example: ADMIN_IDS=123456789,987654321
ADMIN_IDS=

#######################################
# AI MODEL CONFIGURATION
#######################################

# Learning rate for model training (default: 0.1)
LEARNING_RATE=0.1

# Minimum confidence threshold for responses (default: 0.6)
MIN_CONFIDENCE=0.6

#######################################
# BOT BEHAVIOR SETTINGS
#######################################

# Rate limit in messages per second (default: 3)
RATE_LIMIT=3

# Show typing indicator (true/false, default: true)
TYPING_INDICATOR=True

#######################################
# FILE PATHS (Don't modify unless needed)
#######################################
MODEL_PATH=model/ai_model.pth
KNOWLEDGE_FILE=model/knowledge.json
LOG_FILE=logs/bot.log
ERROR_LOG=logs/errors.log